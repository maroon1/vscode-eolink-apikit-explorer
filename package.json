{
  "name": "eolink-apikit-explorer",
  "displayName": "eolink Apikit Explorer",
  "version": "1.1.0",
  "description": "查看 Apikit API 对应的 TypeScript 类型定义",
  "categories": [
    "Other"
  ],
  "keywords": [
    "openapi",
    "eolink",
    "apikit",
    "typescript"
  ],
  "repository": {
    "url": "https://github.com/maroon1/vscode-eolink-apikit-explorer"
  },
  "author": {
    "name": "maroon1",
    "email": "sage2257@outlook.com"
  },
  "publisher": "maroon1",
  "main": "./dist/extension.js",
  "scripts": {
    "compile": "webpack",
    "compile-tests": "tsc -p . --outDir out",
    "lint": "eslint src",
    "package": "webpack --mode production --devtool hidden-source-map",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "test": "vscode-test",
    "vscode:prepublish": "pnpm run package",
    "watch": "webpack --watch",
    "watch-tests": "tsc -p . -w --outDir out"
  },
  "contributes": {
    "commands": [
      {
        "command": "eolinkApikitExplorer.refresh",
        "title": "刷新",
        "icon": "$(refresh)"
      },
      {
        "command": "eolinkApikitExplorer.setup",
        "title": "设置",
        "icon": "$(gear)"
      },
      {
        "command": "eolinkApikitExplorer.search",
        "title": "搜索",
        "icon": "$(search)"
      }
    ],
    "configuration": {
      "id": "eolinkApikitExplorer",
      "title": "eolink Apikit Explorer",
      "properties": {
        "eolinkApikitExplorer.settings": {
          "type": "object",
          "description": "设置项",
          "properties": {
            "origin": {
              "type": "string",
              "description": "Eolinker URL"
            },
            "apiKey": {
              "type": "string",
              "description": "个人访问令牌"
            },
            "workspaceId": {
              "type": "string",
              "description": "工作空间 ID"
            },
            "projectId": {
              "type": "string",
              "description": "项目 ID"
            }
          }
        }
      }
    },
    "menus": {
      "view/title": [
        {
          "command": "eolinkApikitExplorer.search",
          "group": "navigation",
          "when": "view == eolinkApikitExplorer"
        },
        {
          "command": "eolinkApikitExplorer.setup",
          "group": "navigation",
          "when": "view == eolinkApikitExplorer"
        },
        {
          "command": "eolinkApikitExplorer.refresh",
          "group": "navigation",
          "when": "view == eolinkApikitExplorer"
        }
      ]
    },
    "views": {
      "explorer": [
        {
          "id": "eolinkApikitExplorer",
          "name": "eolink Apikit Explorer",
          "contextualTitle": "eolink Apikit Explorer"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "eolinkApikitExplorer",
        "contents": "[添加 Open API](command:eolinkApikitExplorer.setup)"
      }
    ]
  },
  "activationEvents": [],
  "dependencies": {
    "axios": "^1.7.9",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "~1.85.0",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.19.0",
    "prettier": "^3.5.1",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-packagejson": "^2.5.8",
    "ts-loader": "^9.5.2",
    "typescript": "^5.7.3",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "icon": "images/icon.png"
}
